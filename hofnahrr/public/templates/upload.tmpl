<div class="row-fluid">
        <button class="pull-right show-sight-form btn btn-small" title="{{i18n "sight_create"}}"><i class="pictos">^</i> {{#if id}}{{interpolateI18n "sight_edit_name" name}}{{else}}{{i18n "sight_create"}}{{/if}}...</button>
</div>
<div id="upload-preview" class="fixed-fluid">
    <aside class="well">
        <h5>
            {{i18n "sights_sights"}}
            <a class="pull-right pictures-help"><i class="pictos">?</i></a>
        </h5>
        <ul class="nav nav-list unstyled">
            <li class="nav-header">{{i18n "sights_unknown"}}</li>
            <li class="hover-tools-trigger">
                <a data-file-drop-container-id="-1">{{i18n "sight_dont_know"}}</a>
                <span class="hover-tools">
                    <a class="adder" title="{{i18n "sight_add_selected_photos"}}"><i class="pictos">&</i></a>
                </span>
            </li>
            <li class="nav-header">{{i18n "sights_existing"}}</li>
            {{#sightsList}}
            <li class="hover-tools-trigger">
                <a data-file-drop-container-id="{{id}}">{{name}}</a>
                <span class="hover-tools">
                    <a class="adder" title="{{i18n "sight_add_selected_photos"}}"><i class="pictos">&</i></a>
                </span>
            </li>
            {{/sightsList}}
        </ul>
    </aside>
    <div class="row-fluid">
        <ul class="thumbnails">
            <li class="justifier"><!-- --></li>
        </ul>
    </div>
</div>
